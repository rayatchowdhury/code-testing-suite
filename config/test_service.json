{
  "languages": {
    "cpp": {
      "compiler": "g++",
      "standard": "c++17", 
      "optimization": "O2",
      "architecture_flags": ["-march=native"],
      "debug_flags": ["-g"],
      "extra_flags": ["-Wall", "-pipe"],
      "timeout": 30,
      "memory_limit": null
    },
    "python": {
      "compiler": "python3",
      "standard": "3.9",
      "optimization": "",
      "architecture_flags": [],
      "debug_flags": [],
      "extra_flags": [],
      "timeout": 30,
      "memory_limit": null
    },
    "java": {
      "compiler": "javac",
      "standard": "17",
      "optimization": "",
      "architecture_flags": [],
      "debug_flags": [],
      "extra_flags": [],
      "timeout": 30,
      "memory_limit": null
    }
  },
  "execution": {
    "max_parallel_workers": 4,
    "default_test_timeout": 10,
    "default_memory_limit": 256
  },
  "workspace_dir": "workspace/",
  "cache_enabled": true,
  "cleanup_temp_files": true
}